{
  "imports": {
    "prefix": "imports",
    "body": "import (\n\t${1:package}\n)"
  },
  "main function": {
    "prefix": "main",
    "body": "package main\n\nfunc main() {\n\t$1\n}"
  },
  "function": {
    "prefix": "fn",
    "body": "func ${1:name}($2) $3 {\n\t$4\n}"
  },
  "if": {
    "prefix": "if",
    "body": "if $1 {\n\t$2\n}"
  },
  "if error": {
    "prefix": "iferr",
    "body": "if err != nil {\n\t$1\n}"
  },
  "for": {
    "prefix": "for",
    "body": "for ${1:init-statement}; ${2:condition}; ${3:inc-expression} {\n\t$4\n}"
  },
  "type struct": {
    "prefix": "struct",
    "body": "type ${1:name} struct {\n\t$2\n}"
  }
}
