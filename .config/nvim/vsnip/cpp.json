{
  "std": {
    "prefix": ["std"],
    "body": ["std::"]
  },
  "cout": {
    "prefix": ["cout"],
    "body": ["cout << $1 << endl"]
  },
  "header": {
    "prefix": ["header"],
    "body": [
      "#ifndef ${1:name}",
      "#define ${2:name}",
      "",
      "${3:main}",
      "",
      "#endif"
    ]
  }
}
